{
    "compileOnSave": true,
    "compilerOptions": {
        "baseUrl": "src",
        "noImplicitAny": false,
        "noUnusedLocals": false,
        "outDir": "out",
        "preserveSymlinks": true,
        "rootDirs": ["src", "test"],
        "typeRoots": [],
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "preserve",
        "lib": ["es6", "dom", "es2016", "es2017.object", "esnext", "webworker"],
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "plugins": [
            {
                "name": "typescript-styled-plugin"
            }
        ],
        "pretty": true,
        "removeComments": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es5",
        "version": true,
        "esModuleInterop": true,
        "strict": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true
    },
    "exclude": ["node_modules", "build", "dist", "**/__snapshots__/*"],
    "files": ["src/index.tsx"],
    "include": [],
    "references": []
}
